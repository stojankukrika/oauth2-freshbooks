{
  "name": "stojankukrika/oauth2-freshbooks",
  "description": "Freshbook OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Stojan Kukrika",
      "email": "hello@sdmotivation.com",
      "homepage": "https://www.sdmotivation.com"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "freshbooks"
  ],
  "require": {
    "stojankukrika/oauth2-client": "^1.0"
  },
  "require-dev": {
    "php": ">= 7.1",
    "mockery/mockery": "^1.1",
    "squizlabs/php_codesniffer": "^3.2",
    "phpunit/phpunit": "^7.1",
    "satooshi/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Stojankukrika\\OAuth2\\Client\\Provider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stojankukrika\\OAuth2\\Client\\Provider\\Test\\": "tests/src/"
    }
  }
}
